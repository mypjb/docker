apiVersion: gateway.solo.io/v1
kind: VirtualService
metadata:
  name: grafana-ui
  namespace: gloo-system
spec:
  displayName: grafana-ui
  virtualHost:
    domains:
    - grafana.lass.net
    routes:
    - matchers:
      - prefix: /
      routeAction:
        single:
          upstream:
            name: development-grafana-3000
            namespace: gloo-system
